SELECT * FROM ee;

/* COUNTING NUMBER OF FACULTY */
SELECT COUNT(Faculty_ID) AS Faculty FROM faculty;

/* COUNTING NUMBER OF STUDENTS IN EXTC BRANCH */
SELECT COUNT(Student_ID) AS EXTC_Student FROM extc;

/* COUNTING NUMBER OF STUDENTS IN EL BRANCH */
SELECT COUNT(Student_ID) AS EL_Student FROM el;

/* COUNTING NUMBER OF STUDENTS IN CS BRANCH */
SELECT COUNT(Student_ID) AS CS_Student FROM cs;

/* COUNTING NUMBER OF STUDENTS IN MECH BRANCH */
SELECT COUNT(Student_ID) AS MECH_Student FROM mech;

/* COUNTING NUMBER OF STUDENTS IN AIML BRANCH */
SELECT COUNT(Student_ID) AS AIML_Student FROM aiml;

/* COUNTING NUMBER OF STUDENTS IN EE BRANCH */
SELECT COUNT(Student_ID)  AS EE_Student FROM ee;

/* JOINS OPERATION ON FACULTY */
SELECT it.ids, it.branch, faculty.First_Name, faculty.Department, faculty.Faculty_ID
FROM it
JOIN faculty
ON it.ids = faculty.Faculty_ID
ORDER BY it.ids;

SELECT it.ids, it.branch, faculty.First_Name, faculty.Department, faculty.Faculty_ID
FROM it
LEFT JOIN faculty
ON it.ids = faculty.Faculty_ID
ORDER BY it.ids;

SELECT it.ids, it.branch, faculty.First_Name, faculty.Department, faculty.Faculty_ID
FROM it
RIGHT JOIN faculty
ON it.ids = faculty.Faculty_ID
ORDER BY it.ids;

/* JOINS OPERATION ON EXTC BRANCH*/
SELECT it.ids, it.branch, extc.First_Name, extc.Roll_No, extc.Student_ID
FROM it
JOIN extc
ON it.ids = extc.Student_ID;

SELECT it.ids, it.branch, extc.First_Name, extc.Roll_No, extc.Student_ID
FROM it
LEFT JOIN extc
ON it.ids = extc.Student_ID;

SELECT it.ids, it.branch, extc.First_Name, extc.Roll_No, extc.Student_ID
FROM it
RIGHT JOIN extc
ON it.ids = extc.Student_ID;

/* JOINS OPERATION ON EL BRANCH*/
SELECT it.ids, it.branch, el.First_Name, el.Roll_No, el.Student_ID
FROM it
JOIN el
ON it.ids = el.Student_ID;

SELECT it.ids, it.branch, el.First_Name, extc.Roll_No, el.Student_ID
FROM it
LEFT JOIN el
ON it.ids = extc.Student_ID;

SELECT it.ids, it.branch, el.First_Name, el.Roll_No, el.Student_ID
FROM it
RIGHT JOIN el
ON it.ids = el.Student_ID;

/* JOINS OPERATION ON CS BRANCH*/
SELECT it.ids, it.branch, cs.First_Name, cs.Roll_No, cs.Student_ID
FROM it
JOIN cs
ON it.ids = cs.Student_ID;

SELECT it.ids, it.branch, cs.First_Name, cs.Roll_No, cs.Student_ID
FROM it
LEFT JOIN cs
ON it.ids = cs.Student_ID;

SELECT it.ids, it.branch, extc.First_Name, cs.Roll_No, cs.Student_ID
FROM it
RIGHT JOIN cs
ON it.ids = cs.Student_ID;

/* JOINS OPERATION ON MECH BRANCH*/
SELECT it.ids, it.branch, mech.First_Name, mech.Roll_No, mech.Student_ID
FROM it
JOIN mech
ON it.ids = mech.Student_ID;

SELECT it.ids, it.branch, mech.First_Name, mech.Roll_No, mech.Student_ID
FROM it
LEFT JOIN mech
ON it.ids = mech.Student_ID;

SELECT it.ids, it.branch, extc.First_Name, mech.Roll_No, mech.Student_ID
FROM it
RIGHT JOIN mech
ON it.ids = mech.Student_ID;

/* JOINS OPERATION ON AIML BRANCH*/
SELECT it.ids, it.branch, aiml.First_Name, aiml.Roll_No, aiml.Student_ID
FROM it
JOIN aiml
ON it.ids = aiml.Student_ID;

SELECT it.ids, it.branch, aiml.First_Name, aiml.Roll_No, aiml.Student_ID
FROM it
LEFT JOIN aiml
ON it.ids = aiml.Student_ID;

SELECT it.ids, it.branch, aiml.First_Name, aiml.Roll_No, aiml.Student_ID
FROM it
RIGHT JOIN aiml
ON it.ids = aiml.Student_ID;
