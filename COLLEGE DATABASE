
CREATE DATABASE sigce;
USE sigce;

CREATE TABLE it (
    ids VARCHAR(100),
    fname VARCHAR(100),
    lname VARCHAR(100),
    branch VARCHAR(100) DEFAULT 'NOT ASSIGNED',
    PRIMARY KEY(ids)
);

CREATE TABLE faculty (
    fid VARCHAR(10),
    fname VARCHAR(100),
    lname VARCHAR(100),
    branch VARCHAR(100) DEFAULT 'NOT ASSIGNED',
    FOREIGN KEY(fid) REFERENCES it(ids) ON DELETE CASCADE 
);

CREATE TABLE extc (
    sid VARCHAR(10),
    fname VARCHAR(100),
    lname VARCHAR(100),
    rno INT,
    sex VARCHAR(1),
    bday DATE ,
    FOREIGN KEY(sid) REFERENCES it(ids) ON DELETE CASCADE
);

CREATE TABLE el (
    sid VARCHAR(10),
    fname VARCHAR(100),
    lname VARCHAR(100),
    rno INT,
    sex VARCHAR(1),
    bday DATE,
    FOREIGN KEY(sid) REFERENCES it(ids) ON DELETE CASCADE
);

CREATE TABLE cs (
    sid VARCHAR(10),
    fname VARCHAR(100),
    lname VARCHAR(100),
    rno INT,
    sex VARCHAR(1),
    bday DATE,
    FOREIGN KEY(sid) REFERENCES it(ids) ON DELETE CASCADE
);

CREATE TABLE mech (
    sid VARCHAR(10),
    fname VARCHAR(100),
    lname VARCHAR(100),
    rno INT,
    sex VARCHAR(1),
    bday DATE,
    FOREIGN KEY(sid) REFERENCES it(ids) ON DELETE CASCADE
);

CREATE TABLE aiml (
    sid VARCHAR(10),
    fname VARCHAR(100),
    lname VARCHAR(100),
    rno INT,
    sex VARCHAR(1),
    bday DATE,
    FOREIGN KEY(sid) REFERENCES it(ids) ON DELETE CASCADE
);

CREATE TABLE ee (
    sid VARCHAR(10),
    fname VARCHAR(100),
    lname VARCHAR(100),
    rno INT,
    sex VARCHAR(1) ,
    bday DATE,
    FOREIGN KEY(sid) REFERENCES it(ids) ON DELETE CASCADE
);

