/* FIND ALL ELECTRONICS and ELECTRICAL(EE) STUDENTS */
SELECT * FROM ee;

/* TEMPORARY COLUMN NAME CHANGE OF "IT" */
SELECT ids AS IDs, fname AS First_Name, lname AS Last_name, branch AS Department 
FROM it; 

/* PERMANENT CHANGING THE NAME OF COLUMN OF FACULTY */
ALTER TABLE faculty RENAME COLUMN lname TO Last_Name;
ALTER TABLE faculty RENAME COLUMN fname TO First_Name;
ALTER TABLE faculty RENAME COLUMN fid TO Faculty_ID;
ALTER TABLE faculty RENAME COLUMN branch TO Branch;

/* PERMANENT CHANGING THE NAME OF COLUMN OF EXTC BRANCH */
ALTER TABLE extc RENAME COLUMN rno TO Roll_No;
ALTER TABLE extc RENAME COLUMN lname TO Last_Name;
ALTER TABLE extc RENAME COLUMN fname TO First_Name;
ALTER  TABLE extc RENAME COLUMN sid TO Student_ID;
ALTER TABLE extc RENAME COLUMN sex TO Gender;
ALTER TABLE extc RENAME COLUMN bday TO Birthday;

/* PERMANENT CHANGING THE NAME OF COLUMN OF EL BRANCH */
ALTER TABLE el RENAME COLUMN rno TO Roll_No;
ALTER TABLE el RENAME COLUMN lname TO Last_Name;
ALTER TABLE el RENAME COLUMN fname TO First_Name;
ALTER TABLE el RENAME COLUMN sid TO Student_ID;
ALTER TABLE el RENAME COLUMN sex TO Gender;
ALTER TABLE el RENAME COLUMN bday TO Birthday;

/* PERMANENT CHANGING THE NAME OF COLUMN OF CS BRANCH */
ALTER TABLE cs RENAME COLUMN rno TO Roll_No;
ALTER TABLE cs RENAME COLUMN lname TO Last_Name;
ALTER TABLE cs RENAME COLUMN fname TO First_Name;
ALTER TABLE cs RENAME COLUMN sid TO Student_ID;
ALTER TABLE cs RENAME COLUMN sex TO Gender;
ALTER TABLE cs RENAME COLUMN bday TO Birthday;

/* PERMANENT CHANGING THE NAME OF COLUMN OF MECH BRANCH */
ALTER TABLE mech RENAME COLUMN rno TO Roll_No;
ALTER TABLE mech RENAME COLUMN lname TO Last_Name;
ALTER TABLE mech RENAME COLUMN fname TO First_Name;
ALTER TABLE mech RENAME COLUMN sid TO Student_ID;
ALTER TABLE mech RENAME COLUMN sex TO Gender;
ALTER TABLE mech RENAME COLUMN bday TO Birthday;

/* PERMANENT CHANGING THE NAME OF COLUMN OF AIML BRANCH */
ALTER TABLE aiml RENAME COLUMN rno TO Roll_No;
ALTER TABLE aiml RENAME COLUMN lname TO Last_Name;
ALTER TABLE aiml RENAME COLUMN fname TO First_Name;
ALTER TABLE aiml RENAME COLUMN sid TO Student_ID;
ALTER TABLE aiml RENAME COLUMN sex TO Gender;
ALTER TABLE aiml RENAME COLUMN bday TO Birthday;

/* PERMANENT CHANGING THE NAME OF COLUMN OF EE BRANCH */
ALTER TABLE ee RENAME COLUMN rno TO Roll_No;
ALTER TABLE ee RENAME COLUMN lname TO Last_Name;
ALTER TABLE ee RENAME COLUMN fname TO First_Name;
ALTER TABLE ee RENAME COLUMN sid TO Student_ID;
ALTER TABLE ee RENAME COLUMN sex TO Gender;
ALTER TABLE ee RENAME COLUMN bday TO Birthday;
SELECT * FROM ee;

/* UPDATING FACULTY TABLE */
ALTER TABLE faculty RENAME COLUMN Branch TO Department;
